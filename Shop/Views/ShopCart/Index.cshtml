@model ShopCartViewModel
<div class="container">
    @foreach(var el in Model.shopCart.listShopItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Деталь:</b> @el.detail.detailName<br/>

        </div>
    }
    <hr/>
    <a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">Заказать</a>
</div>
